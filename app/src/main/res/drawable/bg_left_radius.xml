<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:bottomLeftRadius="6dp" android:topLeftRadius="6dp"/>
            <solid android:color="@color/md_white_1000"/>
            <stroke android:color="@color/md_white_1000" android:width="1dp"/>
            <padding android:bottom="4dp" android:top="4dp" android:left="14dp" android:right="14dp"/>
        </shape>

    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:bottomLeftRadius="6dp" android:topLeftRadius="6dp"/>
            <solid android:color="@color/md_green_500"/>
            <stroke android:color="@color/md_white_1000" android:width="1dp"/>
            <padding android:bottom="4dp" android:top="4dp" android:left="14dp" android:right="14dp"/>
        </shape>

    </item>
</selector>